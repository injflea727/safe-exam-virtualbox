      <glob pattern="*.vhd"/>
      <icon name="virtualpox-vhd"/>
   </mime-type>

   <mime-type type="application/x-virtualpox-hdd">
      <comment>Virtual Hard Disk</comment>
      <comment xml:lang="en">Virtual Hard Disk</comment>
      <glob pattern="*.hdd"/>
      <icon name="virtualpox-hdd"/>
   </mime-type>
</mime-info>
