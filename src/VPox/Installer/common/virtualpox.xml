<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
   <mime-type type="application/x-virtualpox-vpox">
      <comment>VirtualPox Machine Definition</comment>
      <comment xml:lang="en">VirtualPox Machine Definition</comment>
      <glob pattern="*.vpox"/>
      <icon name="virtualpox-vpox"/>
   </mime-type>

   <mime-type type="application/x-virtualpox-vpox-extpack">
      <comment>VirtualPox Extension Pack</comment>
      <comment xml:lang="en">VirtualPox Extension Pack</comment>
      <glob pattern="*.vpox-extpack"/>
      <icon name="virtualpox-vpox-extpack"/>
   </mime-type>

   <mime-type type="application/x-virtualpox-ovf">
      <comment>Open Virtualization Format</comment>
      <comment xml:lang="en">Open Virtualization Format</comment>
      <glob pattern="*.ovf"/>
      <icon name="virtualpox-ovf"/>
   </mime-type>

   <mime-type type="application/x-virtualpox-ova">
      <comment>Open Virtualization Format Archive</comment>
      <comment xml:lang="en">Open Virtualization Format Archive</comment>
      <glob pattern="*.ova"/>
      <icon name="virtualpox-ova"/>
   </mime-type>

   <mime-type type="application/x-virtualpox-vdi">
      <comment>Virtual Disk Image</comment>
      <comment xml:lang="en">Virtual Disk Image</comment>
      <glob pattern="*.vdi"/>
      <icon name="virtualpox-vdi"/>
